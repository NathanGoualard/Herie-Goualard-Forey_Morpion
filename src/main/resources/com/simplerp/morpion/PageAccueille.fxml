<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.simplerp.morpion.accueille.AccueilleController"
      alignment="CENTER" spacing="30" prefWidth="600" prefHeight="400">
    <Label text="MORPION" styleClass="titre"/>
    <Button text="1 Joueur" styleClass="menu-bouton" onAction="#onJoueur1"/>
    <Button text="2 Joueurs" styleClass="menu-bouton" onAction="#onJoueur2"/>
    <Button text="Scores" styleClass="menu-bouton" onAction="#onScores"/>
</VBox>